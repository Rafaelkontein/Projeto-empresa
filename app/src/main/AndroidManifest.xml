<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjectRafa"
        tools:targetApi="31">
        <activity
            android:name=".ui.Atividade.ActDadosPessoais"
            android:exported="false"
            android:label="@string/title_activity_act_dados_pessoais"
            android:theme="@style/Theme.ProjectRafa" />
        <activity
            android:name=".ui.Atividade.ActAprenstacao"
            android:exported="false"
            android:label="@string/title_activity_act_aprenstacao"
            android:theme="@style/Theme.ProjectRafa" />
        <activity
            android:name=".ui.Atividade.MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.ProjectRafa">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>